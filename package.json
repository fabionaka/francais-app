{
  "name": "francais",
  "version": "1.0.1",
  "author": "Fábio Nakatani",
  "description": "Exercícios de frances",
  "license": "MIT",
  "node-main": "src/backend/server.js",
  "main": "http://localhost:8964",
  "node-remote": "http://localhost:3000",
  "isProd": false,
  "window": {
    "icon": "src/assets/icon.png",
    "title": "Exercices de français",
    "frame": false
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular/animations": "~10.0.2",
    "@angular/cli": "~10.0.1",
    "@angular/common": "~10.0.2",
    "@angular/compiler": "~10.0.2",
    "@angular/compiler-cli": "~10.0.2",
    "@angular/core": "~10.0.2",
    "@angular/forms": "~10.0.2",
    "@angular/platform-browser": "~10.0.2",
    "@angular/platform-browser-dynamic": "~10.0.2",
    "@angular/router": "~10.0.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0-next.1",
    "concurrently": "^6.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nw": "^0.54.0-sdk",
    "nwjs-builder-phoenix": "^1.15.0",
    "nwjs-types": "^1.0.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "~6.5.5",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "wait-on": "^5.3.0",
    "zone.js": "~0.10.3"
  },
  "scripts": {
    "start": "nw .",
    "start:dev": "run --x64 --mirror https://dl.nwjs.io/ .",
    "dist": "build --tasks mac-x64 --mirror https://dl.nwjs.io/ .",
    "dev": "concurrently \"cd src/frontend && npm run start:web\" \"wait-on http://localhost:8964 && nw .\"",
    "build": "npm run build:clean && npm run build:ng && npm run build:package && npm run build:backend && npm run build:nw",
    "build:clean": "rimraf ./dist",
    "build:ng": "cd ./src/frontend && npm run prod",
    "build:package": "node ./build.package.js",
    "build:backend": "cp -R ./src/backend ./dist/backend && cp -R ./src/assets ./dist/assets && node build.backend.js",
    "build:nw": "cd dist && pwd && npm run dist"
  },
  "build": {
    "nwVersion": "0.54.0",
    "targets": [
      "zip",
      "nsis7z"
    ],
    "win": {
      "icon": "src/assets/icon.ico"
    },
    "mac": {
      "icon": "src/assets/icon.icns"
    }
  }
}
